{
  "kind": "collectionType",
  "collectionName": "activities",
  "info": {
    "singularName": "activity",
    "pluralName": "activities",
    "displayName": "Activity",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "StartActivity": {
      "type": "date"
    },
    "EndActivity": {
      "type": "datetime"
    },
    "StartRegistration": {
      "type": "date"
    },
    "EndRegistration": {
      "type": "datetime"
    },
    "Number": {
      "type": "integer"
    },
    "Contact": {
      "type": "text"
    },
    "catagories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::catagory.catagory",
      "inversedBy": "activities"
    },
    "Typeregister": {
      "type": "enumeration",
      "enum": [
        "First Come First Serve",
        "Candidate"
      ]
    },
    "FirstComeFirstServe": {
      "type": "component",
      "repeatable": true,
      "component": "typeregister.type1"
    },
    "Candidate": {
      "type": "component",
      "repeatable": true,
      "component": "typeregister.type2"
    }
  }
}
